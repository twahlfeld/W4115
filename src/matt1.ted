int main() {
  Element e;
  Element e1;
  List list;
  string s;
  string s1;
  Page p = pageFetch(“http://example.com”); /* the final variable declaration can use a      function */
  e = pageRoot(p); /* pageRoot will usually be the “head” element” */
  list = elementChildren(e);
  e1 = listHead(list);
  s1 = elementType(e1);
  print(stdout, “%s/n%s/n”, s, s1);
  return 0;
}
